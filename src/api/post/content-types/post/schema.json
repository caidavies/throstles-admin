{
  "kind": "collectionType",
  "collectionName": "posts",
  "info": {
    "singularName": "post",
    "pluralName": "posts",
    "displayName": "Post",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "maxLength": 70
    },
    "featured_image": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "content": {
      "type": "blocks"
    },
    "premium": {
      "type": "boolean",
      "default": true,
      "required": true
    },
    "slug": {
      "type": "uid"
    },
    "subtitle": {
      "type": "string",
      "required": true
    },
    "author": {
      "type": "string",
      "required": true
    }
  }
}
